{
  "name": "@donmahallem/kvg-server",
  "description": "Angular Client for kvg-server",
  "version": "0.0.1",
  "scripts": {
    "test": "mocha --config ./.mocharc.yml",
    "test:watch": "mocha --watch",
    "start": "npm run build:live",
    "build:live": "nodemon",
    "build": "tsc",
    "test:coverage": "nyc --nycrc-path ./.nycrc.json npm run test",
    "lint": "tslint -c tslint.json -p tsconfig.json src/**/*.ts",
    "lint:fix": "tslint --fix -c tslint.json -p tsconfig.json src/**/*.ts",
    "docs": "typedoc --options typedoc.json",
    "commit": "git-cz"
  },
  "private": true,
  "homepage": "https://donmahallem.github.io/kvg-server/",
  "author": {
    "name": "donmahallem",
    "email": "donmahallem@users.noreply.github.com",
    "url": "https://github.com/donmahallem"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/donmahallem/kvg-server/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/donmahallem/kvg-server.git"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "publishConfig": {
    "access": "private"
  },
  "dependencies": {
    "@manniwatch/api-client": "~0.16.0",
    "@manniwatch/api-proxy-router": "~0.15.0",
    "express": "^4.17.1",
    "helmet": "^4.1.0"
  },
  "devDependencies": {
    "@types/chai": "~4.2.12",
    "@types/mocha": "~8.0.3",
    "@types/node": "~14.6.0",
    "@types/sinon": "~9.0.4",
    "chai": "~4.2.0",
    "commitizen": "~4.1.2",
    "cz-conventional-changelog": "~3.2.0",
    "mocha": "~8.1.1",
    "nodemon": "^2.0.4",
    "nyc": "~15.1.0",
    "sinon": "~9.0.3",
    "ts-node": "~8.10.2",
    "tslint": "6.1.3",
    "typescript": "~3.9.7"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
